{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist/",
    "rootDir": "./", 
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["./src/**/*.ts", "./prisma/**/*.ts"],
  "exclude": ["node_modules"]
}