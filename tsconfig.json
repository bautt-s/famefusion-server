  "compilerOptions": {
    "outDir": "./dist/",
    "target": "ES2015",
    "module": "NodeNext",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": ["./src/**/*.ts", "./prisma/**/*.ts", "./index.ts"],
  "exclude": ["node_modules"]
}